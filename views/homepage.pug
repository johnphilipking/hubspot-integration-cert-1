
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body
        h1 #{title}

         h2 Custom Object Table

        a(href="/update-cobj") Add/Update This Table
        table.table
            thead
                tr
                th Name
                th Artist Type
                th Artist Work
                tbody
                each artist in artists
                    tr
                        td #{artist.properties.name}
                        td #{artist.properties.artist_type}
                        td #{artist.properties.artist_work}
                